  "author": "Tiny Technologies",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@ephox/agar": "^6.0.3",
    "@ephox/bedrock-client": "^13.0.0",
    "@ephox/bedrock-server": "^13.0.0",
    "@ephox/mcagar": "^7.0.4",
    "@ephox/sugar": "^8.1.1",
    "@ephox/swag": "^4.5.0",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/html": "^6.4.14",
    "@storybook/storybook-deployer": "^2.8.10",
    "@tinymce/beehive-flow": "^0.17.0",
    "@tinymce/eslint-plugin": "^2.0.1",
    "@types/jquery": "^3.5.13",
    "babel-loader": "^8.2.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.0",
    "tinymce": "^5.5.1",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
