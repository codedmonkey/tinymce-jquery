  "author": "Tiny Technologies",
  "license": "Apache-2.0",
  "private": true,
  "devDependencies": {
    "@ephox/agar": "^4.17.2",
    "@ephox/bedrock-client": "^9.6.1",
    "@ephox/bedrock-server": "^9.6.3",
    "@ephox/sugar": "^6.1.3",
    "@ephox/swag": "^4.3.1",
    "@types/jquery": "^3.5.1",
    "@types/tinymce": "^4.5.24",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.11",
    "ts-loader": "^8.0.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
